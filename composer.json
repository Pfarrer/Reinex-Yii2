{
	"name": "Reinex website",
	"description": "Corporate website",
	"license": "proprietary",
	"type": "project",
	"hompage": "http://reinex.de",
	"authors": [
		{
			"name": "Brian Pfretzschner",
			"email": "brian.pfretzschner@gmail.com"
		}
	],
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.4.0",
		"yiisoft/yii2": "2.0.4",
		"yiisoft/yii2-bootstrap": "2.0.4",
		"rmrevin/yii2-minify-view": "~1.8",
		"kartik-v/yii2-widgets": "dev-master",
		"netcarver/textile": "v3.5.5",
		"xj/yii2-fullpage-widget": "*",
		"smottt/wideimage": "*",
		"2amigos/yii2-file-upload-widget": "~1.0",
		"kartik-v/yii2-sortable": "dev-master",
		"pfarrer/yii2-email-obfuscator": "dev-master"
	},
	"require-dev": {
		"yiisoft/yii2-debug": "*",
		"raveren/kint": "dev-master"
	},
	"autoload": {
		"psr-0": {
			"app": ""
		}
	},
	"scripts": {
		"post-create-project-cmd": [
			"yii\\composer\\Installer::setPermission"
		]
	},
	"extra": {
		"asset-installer-paths": {
			"npm-asset-library": "vendor/npm",
			"bower-asset-library": "vendor/bower"
		},
		"writable": [
			"runtime",
			"web/assets",
			"web/uploads"
		],
		"executable": [
			"yii"
		]
	},
	"config": {
		"process-timeout": 1800
	}
}